<?xml version="1.0" encoding="UTF-8"?>
<ServerConfig name="TexasPokerHall">
	<Enums>
		<LanguageS>
			<LAN_zh_CN/>
      <LAN_zh_TW/>
      <LAN_en_US/>
		</LanguageS>
		<LoginResult>
      <LOGIN_SUCCEED/>
			<LOGIN_CREATE/>
			<LOGIN_EXIST/>
			<LOGIN_WRONG_VERSION/>
			<LOGIN_FAILED/>
		</LoginResult>
    <RoomType>
      <ROOM_TYPE_PRACTICE/>
      <ROOM_TYPE_NORMAL/>
      <ROOM_TYPE_EXPERT/>
      <ROOM_TYPE_VIP/>
      <ROOM_TYPE_CHAMPIONSHIP/>
      <ROOM_TYPE_KNOCKOUT/>
    </RoomType>
    <RoomFlag>
      <ROOM_FLAG_VIP value="0x1"/>
      <ROOM_FLAG_HOT value="0x2"/>
    </RoomFlag>
    <EditInfoRes>
      <EDIT_S_OK/>
      <EDIT_E_FAIL/>
    </EditInfoRes>
    <ExchangeRes>
      <EXCHANGE_S_OK/>
      <EXCHANGE_E_FAIL/>
    </ExchangeRes>
    <KenoRes>
      <KENO_S_OK/>
      <KENO_E_FAIL/>
    </KenoRes>
    <BaccaratBetType>
      <BACC_BANKER/>
      <BACC_PLAYER/>
      <BACC_TIE/>
    </BaccaratBetType>
    <BaccaratRes>
      <BACC_S_OK/>
      <BACC_E_FAIL/>
    </BaccaratRes>
    <EnterTableRes>
      <ENTER_S_OK/>
      <ENTER_E_FAIL/>
      <ENTER_NO_CHIPS/>
      <ENTER_NOT_VIP/>
      <ENTER_HAS_TABLE/>
    </EnterTableRes>
    <TableState>
      <TABLE_WAIT/>
      <TABLE_DEAL_0/>
      <TABLE_DEAL_1/>
      <TABLE_DEAL_2/>
      <TABLE_DEAL_3/>
      <TABLE_PLAY_0/>
      <TABLE_PLAY_1/>
      <TABLE_PLAY_2/>
      <TABLE_PLAY_3/>
      <TABLE_CLEAR/>
      <TABLE_ALL_FOLD/>
    </TableState>
    <TablePlayerState>
      <TABLE_PLAYER_DISABLE/>
      <TABLE_PLAYER_HOLD/>
      <TABLE_PLAYER_READY/>
      <TABLE_PLAYER_PLAY/>
      <TABLE_PLAYER_FOLD/>
      <TABLE_PLAYER_RAISE/>
      <TABLE_PLAYER_CALL/>
      <TABLE_PLAYER_CHECK/>
      <TABLE_PLAYER_ALL_IN/>
      <TABLE_PLAYER_BIG_BLIND/>
      <TABLE_PLAYER_SMALL_BLIND/>
    </TablePlayerState>
    <CardNum>
      <CARD_A/>
      <CARD_2/>
      <CARD_3/>
      <CARD_4/>
      <CARD_5/>
      <CARD_6/>
      <CARD_7/>
      <CARD_8/>
      <CARD_9/>
      <CARD_10/>
      <CARD_J/>
      <CARD_Q/>
      <CARD_K/>
    </CardNum>
    <CardSuit>
      <CARD_SPADE/>
      <CARD_HEART/>
      <CARD_CLUB/>
      <CARD_DIAMOND/>
    </CardSuit>
    <Categories>
      <CATE_ROYAL_FLUSH/>
      <CATE_STRAIGHT_FLUSH/>
      <CATE_FOUR_KIND/>
      <CATE_FULL_HOUSE/>
      <CATE_FLUSH/>
      <CATE_STRAIGHT/>
      <CATE_THREE_KIND/>
      <CATE_TWO_PAIR/>
      <CATE_ONE_PAIR/>
      <CATE_HIGH_CARD/>
    </Categories>
    <RefreshPurchaseRes>
      <PURCHASE_S_OK/>
      <PURCHASE_E_FAIL/>
    </RefreshPurchaseRes>
    <EventInfo>
      <EVENT_MODIFY/>
      <EVENT_PURCHASE/>
      <EVENT_VIP/>
      <EVENT_PRESENT/>
      <EVENT_ONLINE/>
      <EVENT_PLAY_COUNT/>
      <EVENT_PLAY_SLOT_COUNT/>
      <EVENT_PLAY_KENO_COUNT/>
      <EVENT_PLAY_BACC_COUNT/>
      <EVENT_WIN_COMBO_0/>
      <EVENT_WIN_COMBO_1/>
      <EVENT_WIN_COMBO_2/>
    </EventInfo>
    <EventAwardType>
      <EVENT_AWARD_CHIPS/>
      <EVENT_AWARD_GOLD/>
      <EVENT_AWARD_CARD/>
      <EVENT_AWARD_VIP/>
    </EventAwardType>
  </Enums>
	<Structs>
		<RoomInfo>
			<Type type="RoomType"/>
      <SB type="uint32"/>
      <BB type="uint32"/>
      <ChipsMin type="uint32"/>
      <ChipsMax type="uint32"/>
			<Flags type="uint8"/>
		</RoomInfo>
    <CardData>
      <Number type="CardNum"/>
      <Suit type="CardSuit"/>
    </CardData>
    <Table>
      <State type="TableState"/>
      <PlayerIndex type="VeVector&lt;VeUInt8&gt;"/>
      <Dealer type="uint8"/>
      <SmallBlind type="uint8"/>
      <Process type="uint8"/>
      <BetFirst type="uint8"/>
      <BetChips type="uint32"/>
      <Pool type="VeVector&lt;VeUInt32&gt;"/>
      <ProcessExpire type="uint64"/>
      <CardList type="VeVector&lt;CardData&gt;"/>
      <HandCardList type="VeVector&lt;CardData&gt;"/>
    </Table>
    <TablePlayer>
      <Index type="uint32"/>
      <Name type="string"/>
      <Chips type="uint32"/>
      <ChipsOnTable type="uint32"/>
      <State type="TablePlayerState"/>
      <HeadIcon type="uint8"/>
      <Sex type="uint8"/>
      <Vip type="uint8"/>
      <PoolLevel type="uint8"/>
      <Drink type="uint8"/>
    </TablePlayer>
	</Structs>
	<Global name="HallGlobal">
    <Properties>
      <JPChips type="uint64" default="800000000"/>
      <SlotJackPot type="int64"/>
      <KenoJackPot type="int64"/>
      <KenoIssue type="uint32" default="1"/>
      <BaccaratJackPot type="int64"/>
      <Baccarat type="uint32"/>
      <EventTick type="uint32"/>
      <PokerJackPot type="int64"/>
    </Properties>
		<ServerMethods>
			<ReqLogin>
				<PlayerUUID type="string"/>
        <Channel type="string"/>
        <DeviceName type="string"/>
        <Language type="LanguageS"/>
        <VersionMajor type="uint8"/>
        <VersionMinor type="uint8"/>
			</ReqLogin>
      <ReqSyncTime/>
		</ServerMethods>
		<ClientMethods>
			<RecLogin>
				<Result type="LoginResult"/>
			</RecLogin>
      <RecSyncTime>
        <Time type="uint64"/>
      </RecSyncTime>
		</ClientMethods>
	</Global>
	<Entities>
		<Player>
			<Properties key="PlayerUUID">
				<PlayerUUID type="string" sync="false"/>
        <Channel type="string" sync="false"/>
				<Nickname type="string"/>
				<Chips type="uint64" default="5000"/>
				<GoldBricks type="uint64"/>
        <HeadIcon type="uint32"/>
        <Sex type="uint32"/>
				<Title type="uint32"/>
				<Vip type="uint32"/>
				<BankCard type="uint32"/>
        <BankCardExpire type="uint64"/>
				<Obj0 type="uint32"/>
				<Obj1 type="uint32"/>
				<Obj2 type="uint32"/>
				<Obj3 type="uint32"/>
				<Medal0 type="uint32"/>
				<Medal1 type="uint32"/>
				<Medal2 type="uint32"/>
        <PlayChips type="int64" sync="false"/>
				<PlayCount type="uint32"/>
				<WinCount type="uint32"/>
				<WinMaxBet type="uint32"/>
				<BestType type="string"/>
        <Phone type="string"/>
        <SlotJackPot type="int64" sync="false"/>
        <SlotCount type="uint32"/>
        <KenoJackPot type="int64" sync="false"/>
        <KenoCount type="uint32"/>
        <KenoResult type="string" default="0"/>
        <BaccaratJackPot type="int64" sync="false"/>
        <BaccaratCount type="uint32"/>
        <BaccaratResult type="string" default="0"/>
        <Purchase type="uint64"/>
        <PurchaseChips type="uint64"/>
        <PurchaseCount type="uint32"/>
        <EventModifyInfo type="uint32"/>
        <EventPurchase type="uint32"/>
        <EventVip type="uint32"/>
        <EventPresent type="uint32"/>
        <EventTickCurrent type="uint32"/>
        <EventTickOnline type="uint32"/>
        <EventTickPlayCount type="uint32"/>
        <EventTickPlaySlotCount type="uint32"/>
        <EventTickPlayKenoCount type="uint32"/>
        <EventTickPlayBaccCount type="uint32"/>
        <EventTickPlayWinCombo0 type="uint32"/>
        <EventTickPlayWinCombo1 type="uint32"/>
        <EventTickPlayWinCombo2 type="uint32"/>
        <TickPlayCount type="uint32"/>
        <TickPlaySlotCount type="uint32"/>
        <TickPlayKenoCount type="uint32"/>
        <TickPlayBaccCount type="uint32"/>
        <TickPlayWinCombo0 type="uint32"/>
        <TickPlayWinCombo1 type="uint32"/>
        <TickPlayWinCombo2 type="uint32"/>
        <PresentSend type="uint32"/>
			</Properties>
			<ServerMethods>
				<ReqInitHall/>
        <ReqTickHall/>
        <ReqEditInfo>
          <Nickname type="string"/>
          <Sex type="uint8"/>
          <HeadIcon type="uint8"/>
          <Phone type="string"/>
        </ReqEditInfo>
        <ReqGetObjExchangePrice/>
        <ReqExchangeObj>
          <Index type="uint32"/>
        </ReqExchangeObj>
        <ReqSlotBonusCard/>
        <!--Slot begin-->
        <ReqSlotJP/>
        <ReqSlotRun>
          <BetCount type="uint32"/>
        </ReqSlotRun>
        <!--Slot end-->
        <!--Keno begin-->
        <ReqKenoUpdate/>
        <ReqBuyKeno>
          <Bet type="uint8"/>
          <PlayerNumber type="VeVector&lt;VeUInt8&gt;"/>
        </ReqBuyKeno>
        <ReqKenoDraw/>
        <!--Keno end-->
        <!--Baccarat begin-->
        <ReqBaccaratUpdate/>
        <ReqBaccaratBet>
          <BetType type="BaccaratBetType"/>
          <Chips type="uint32"/>
        </ReqBaccaratBet>
        <ReqBaccaratBetAll>
          <Banker type="uint32"/>
          <Player type="uint32"/>
          <Tie type="uint32"/>
        </ReqBaccaratBetAll>
        <ReqBaccaratPlay/>
        <!--Baccarat end-->
        <!--Poker begin-->
        <ReqEnterPokerTable>
          <Index type="uint32"/>
        </ReqEnterPokerTable>
        <ReqLeavePokerTable/>
        <ReqChangeSeat>
          <Index type="uint32"/>
        </ReqChangeSeat>
        <ReqAddChips/>
        <ReqChangeRoom/>
        <ReqReady>
          <Chips type="uint32"/>
        </ReqReady>
        <ReqFold/>
        <ReqCall/>
        <ReqFill>
          <Chips type="uint32"/>
        </ReqFill>
        <ReqBuy>
          <Type type="uint8"/>
          <Index type="uint8"/>
        </ReqBuy>
        <ReqBuyAll>
          <Type type="uint8"/>
        </ReqBuyAll>
        <!--Poker end-->
        <!--Purchase begin-->
        <ReqRefreshPurchase/>
        <!--Purchase end-->
        <!--Event begin-->
        <ReqFinishEvent>
          <Info type="EventInfo"/>
        </ReqFinishEvent>
        <!--Event end-->
			</ServerMethods>
			<ClientMethods>
        <RecInitHall/>
        <UpdateRoomList>
          <RoomList type="VeVector&lt;RoomInfo&gt;"/>
          <DrinkPrice type="VeVector&lt;VeUInt32&gt;"/>
          <ObjPrice type="VeVector&lt;VeUInt32&gt;"/>
        </UpdateRoomList>
        <UpdateOnline>
          <Number type="uint32"/>
        </UpdateOnline>
        <RecEditInfo>
          <Result type="EditInfoRes"/>
        </RecEditInfo>
        <RecGetObjExchangePrice>
          <ExchangePriceList type="VeVector&lt;VeUInt32&gt;"/>
        </RecGetObjExchangePrice>
        <RecExchangeObj>
          <Result type="EditInfoRes"/>
        </RecExchangeObj>
        <RecSlotBonusCard>
          <Bonus type="uint32"/>
        </RecSlotBonusCard>
        <!--Slot begin-->
        <RecSlotJP>
          <JPChips type="uint64"/>
        </RecSlotJP>
        <RecSlotRun>
          <WinChips type="uint64"/>
          <Result0 type="uint8"/>
          <Result1 type="uint8"/>
          <Result2 type="uint8"/>
        </RecSlotRun>
        <!--Slot end-->
        <!--Keno begin-->
        <RecKenoUpdate>
          <IssueNumber type="uint32"/>
          <ExpireTime type="uint64"/>
        </RecKenoUpdate>
        <RecBuyKeno>
          <Result type="KenoRes"/>
        </RecBuyKeno>
        <RecKenoDraw>
          <Result type="KenoRes"/>
          <Hit type="uint8" default="0"/>
          <Issue type="uint32" default="0"/>
          <Bet type="uint32" default="0"/>
          <Win type="uint64" default="0"/>
        </RecKenoDraw>
        <!--Keno end-->
        <!--Baccarat begin-->
        <RecBaccaratUpdate>
          <Index type="uint32"/>
          <ExpireTime type="uint64"/>
          <AllBetPlayer type="uint32"/>
          <AllBetChips type="uint64"/>
        </RecBaccaratUpdate>
        <RecBaccaratBet>
          <Result type="BaccaratRes"/>
        </RecBaccaratBet>
        <RecBaccaratPlay>
          <Result type="BaccaratRes"/>
          <WinChips type="uint32"/>
        </RecBaccaratPlay>
        <!--Baccarat end-->
        <!--Poker begin-->
        <UpdateRoomInfo>
          <Info type="RoomInfo"/>
        </UpdateRoomInfo>
        <UpdateTableSeats>
          <SeatIndex type="uint8"/>
          <Players type="VeVector&lt;TablePlayer&gt;"/>
        </UpdateTableSeats>
        <UpdateTable>
          <Tabel type="Table"/>
        </UpdateTable>
        <UpdateHandCards>
          <HandCards type="VeVector&lt;CardData&gt;"/>
        </UpdateHandCards>
        <RecEnterPokerTable>
          <Result type="EnterTableRes"/>
        </RecEnterPokerTable>
        <OnTableUpdate/>
        <OnTableUpdateNoMotion/>
        <OnBuy>
          <Type type="uint8"/>
          <From type="uint8"/>
          <To type="uint8"/>
        </OnBuy>
        <OnBuyAll>
          <Type type="uint8"/>
          <From type="uint8"/>
        </OnBuyAll>
        <!--Poker end-->
        <!--Purchase begin-->
        <OnPurchaseCompleted>
          <Channel type="string"/>
          <Chips type="uint32"/>
        </OnPurchaseCompleted>
        <RecRefreshPurchase>
          <Result type="RefreshPurchaseRes"/>
        </RecRefreshPurchase>
        <!--Purchase end-->
        <!--Event begin-->
        <RecFinishEvent>
          <Type type="EventAwardType"/>
          <Value type="uint32"/>
        </RecFinishEvent>
        <!--Event end-->
        <!--Notice begin-->
        <RecNotice>
          <Notice type="string"/>
          <Show type="uint16"/>
          <Interval type="uint16"/>
        </RecNotice>
        <!--Notice end-->
      </ClientMethods>
		</Player>
	</Entities>
</ServerConfig>
